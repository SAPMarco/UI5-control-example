	xmlns:l="sap.ui.layout"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:customInput="com.mrb.customcontrol.controls">
	<App id="idAppControl">
		<pages>
			<Page title="{i18n>title}">
				<content>
					<FlexBox height="100%" alignItems="Center" justifyContent="Center">
